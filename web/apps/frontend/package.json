{
  "name": "@language-learner/frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@google/genai": "^1.15.0",
    "@google/generative-ai": "^0.24.1",
    "@language-learner/core": "workspace:*",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.13",
    "fluent-ffmpeg": "^2.1.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.7.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "stream-json": "^1.9.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:contract:consumer": "jest --config jest.config.contracts.js",
    "test:contract:all": "npm run test:contract:consumer",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "performance:audit": "npm run build && npm run analyze",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "type-check": "echo 'No TypeScript in this project'",
    "clean": "rm -rf build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@pact-foundation/pact": "^15.0.1",
    "@testing-library/jest-dom": "^6.8.0",
    "axios": "^0.27.2",
    "babel-jest": "^30.1.2",
    "cors": "^2.8.5",
    "dotenv-cli": "^10.0.0",
    "http-proxy-middleware": "^3.0.5",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "supertest": "^7.1.4",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
